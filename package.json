{
  "name": "captain-pasty",
  "version": "1.0.0",
  "description": "A pastebin for App Engine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidwtbuxton/captain-pasty.git"
  },
  "dependencies": {
    "bulma": "^0.1.2",
    "font-awesome": "^4.6.3",
    "jquery": "~3.1.0"
  },
  "devDependencies": {
    "node-sass": "^3.9.3"
  },
  "scripts": {
    "build": "node-sass --include-path static/src/bulma --include-path static/src/font-awesome/scss static/src/styles.sass static/styles.css",
    "watch": "node-sass --include-path static/src/bulma --include-path static/src/font-awesome/scss --watch static/src/styles.sass static/styles.css"
  }
}
