{
  "name": "captain-pasty",
  "version": "1.0.0",
  "description": "A pastebin for App Engine",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidwtbuxton/captain-pasty.git"
  },
  "dependencies": {
    "bulma": "~0.4.2",
    "font-awesome": "~4.6.3",
    "jquery": "~3.1.1"
  },
  "devDependencies": {
    "node-sass": "~3.9.3",
    "uglify-js": "~2.7.4"
  },
  "scripts": {
    "build": "node-sass --include-path node_modules/bulma --include-path node_modules/font-awesome/scss static/src/styles.scss static/styles.css",
    "watch": "node-sass --include-path node_modules/bulma --include-path node_modules/font-awesome/scss --watch static/src/styles.scss static/styles.css",
    "uglify": "uglifyjs static/src/jquery/dist/jquery.min.js static/src/app.js -o static/app.min.js"
  }
}
